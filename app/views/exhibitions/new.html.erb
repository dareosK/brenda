<%= simple_form_for(@exhibition) do |f| %>
  <%= f.input :title %>
  <%= f.input :artists %>
  <%= f.input :institution_name %>
  <%= f.input :institution_address %>
  <%= f.input :curated_by %>
  <%= f.input :text %>
  <%= f.input :start_date %>
  <%= f.input :end_date %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.submit %>
<% end %>
